@incollection{Petta2001,
abstract = {The encounter between emotion research and agent-based technology is multifaceted. One the one hand, results from emotion
research start to serve as role model from nature, providing inspirations for technical design criteria for individual agents
at the micro level and agent groups and societies at the macro level as well as the sophisticated linkages in between them.
