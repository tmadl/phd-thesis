issn = {1532-5946},
journal = {Memory \& cognition},
keywords = {Adult,Humans,Judgment,Judgment: physiology,Memory, Long-Term,Memory, Long-Term: physiology,Mental Recall,Mental Recall: physiology,Orientation,Orientation: physiology,Space Perception,Space Perception: physiology,Young Adult},
