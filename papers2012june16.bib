@article{Churchland2008,
abstract = {Simple perceptual tasks have laid the groundwork for understanding the neurobiology of decision making. Here, we challenge this foundation to explain how decision-making circuitry adjusts to a more difficult task. We measured behavioral and physiological responses on a 2- and 4-choice direction discrimination decision task. For both tasks, firing rates in the lateral intraparietal area appeared to reflect the accumulation of evidence for or against each choice. Evidence accumulation began at a lower firing rate for the 4-choice task, but reached a common level at the end of the decision process. The larger excursion suggests that subjects required more evidence before making a choice. Further, on both tasks, we observed a time-dependent rise in firing rates that may impose a deadline for deciding. These physiological observations constitute an effective strategy for handling increased task difficulty. The differences appear to explain subjects’ accuracy and reaction times.},
author = {Churchland, Anne K. and Kiani, Roozbeh and Shadlen, Michael N.},
doi = {10.1038/nn.2123},
issn = {1097-6256},
journal = {Nature neuroscience},
month = jun,
number = {6},
pages = {693--702},
title = {{Decision-making with multiple alternatives}},
url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC2453226/},
volume = {11},
year = {2008}
}
