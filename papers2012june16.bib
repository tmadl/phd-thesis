journal = {Nature neuroscience},
keywords = {Action Potentials,Action Potentials: physiology,Animals,Anisotropy,Brain Mapping,Electrodes, Implanted,Entorhinal Cortex,Entorhinal Cortex: cytology,Hippocampus,Hippocampus: cytology,Neurons,Neurons: physiology,Orientation,Orientation: physiology,Rats,Space Perception,Space Perception: physiology},
month = sep,
