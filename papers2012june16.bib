keywords = {Animals,Brain Mapping,Brain Mapping: methods,Cognition,Cognition: physiology,Hippocampus,Hippocampus: physiology,Humans,Nerve Net,Nerve Net: physiology,Neural Pathways,Neural Pathways: physiology},
